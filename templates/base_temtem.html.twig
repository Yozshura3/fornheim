<!doctype html>
<html lang="fr">
<head>
    <meta charset="UTF-8">
    <meta name="viewport"
          content="width=device-width, user-scalable=no, initial-scale=1.0, maximum-scale=1.0, minimum-scale=1.0">
    <meta http-equiv="X-UA-Compatible" content="ie=edge">
    <link href="https://fonts.googleapis.com/css?family=IM+Fell+English+SC&display=swap" rel="stylesheet">
    <link href="https://fonts.googleapis.com/css?family=Rubik&display=swap" rel="stylesheet">
    {% block stylesheet %}{% endblock %}
    <title>{% block title %}{% endblock %}</title>
</head>
<body>
<header>

    <div class="head-wrap">

        <div class="fornheimlogo"><img class="resp" src="{{ asset('img/horizon.png') }}" alt=""></div>
        <a href="{{ path('lol_accueil') }}">
            <div class="redirect">
                <div class="logolol"><img class="resp" src="{{ asset('img/lol_logo2.png') }}" alt=""></div>
                <div class="arrow"><img class="resp" src="{{ asset('img/next_temtem.png') }}" alt=""></div>
            </div></a>

    </div>

    <div class="nav-wrap">
        <a href=""><div class="news">News</div></a>
        <a href=""><div class="guides">Guides</div></a>
        <a href=""><div class="wikitem">Wiki-Temtem</div></a>
        <a href=""><div class="map-menu">Temtem Academia</div></a>
        <a href=""><div class="housing">Housing builder</div></a>
        <a href=""><div class="discord">Serveur discord</div></a>
    </div>
</header>
<div class="searchbar">
    <div class="magnifying"><img class="resp" src="{{ asset('img/search.png') }}" alt=""></div>
    <input class="search" type="search">
    <div class="user"><img class="resp" src="{{ asset('img/user.png') }}" alt=""></div>
    <div class="login">Connexion</div>
</div>

<div class="container">
    {% block section %}

    {% endblock %}
</div>



</body>
</html>